{
  "name": "starter",
  "description": "A starter theme for OLC sites",
  "repository": {
    "type": "git",
    "url": "https://github.com/thelearninghouse/starter-theme.git"
  },
  "version": "0.1.0",
  "scripts": {
    "dev": "yarn run:dev",
    "build": "yarn run:build",
    "build:critical": "yarn run:criticalcss",
    "run:dev": "node node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "run:build": "node node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "run:criticalcss": "node critical.js"
  },
  "devDependencies": {
    "browser-sync": "^2.23.2",
    "browser-sync-webpack-plugin": "^1.2.0",
    "cross-env": "^5.1.5",
    "glob-all": "^3.1.0",
    "laravel-mix": "^2.1.11",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "susy": "^2.2.12"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "critical": "^1.1.0",
    "fontfaceobserver": "^2.0.1",
    "grunt": "^1.0.2",
    "load-grunt-tasks": "~2.0.0",
    "mixitup": "^3.2.1",
    "owl.carousel": "^2.2.0",
    "pluralize": "^7.0.0",
    "priority-nav": "^1.0.12",
    "stickyfilljs": "^2.0.3",
    "vue": "^2.5.16",
    "vue-affix": "^0.2.4",
    "vue-stash": "^2.0.1-beta",
    "vue2-transitions": "^0.2.3",
    "vueisotope": "^3.1.1",
    "wpapi": "^1.1.2"
  },
  "license": "MIT"
}
